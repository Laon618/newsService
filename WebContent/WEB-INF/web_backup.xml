<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemalocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
	<display-name>newsService</display-name>
	
	<servlet>
		<servlet-name>addReview</servlet-name>
		<servlet-class>net.newsService.addReview</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addReview</servlet-name>
		<url-pattern>/addReview</url-pattern>
	</servlet-mapping>
	
	
	<welcom-file-list>
		<welcom-file>index.html</welcom-file>
		<welcom-file>index.htm</welcom-file>
		<welcom-file>index.jsp</welcom-file>
		<welcom-file>default.html</welcom-file>
		<welcom-file>default.htm</welcom-file>
		<welcom-file>default.jsp</welcom-file>
		
	</welcom-file-list>

</web-app>